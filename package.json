{
  "name": "resonance",
  "private": true,
  "version": "0.7.3",
  "description": "Data-driven transitions in React",
  "main": "index.js",
  "repository": "git+https://github.com/sghall/resonance.git",
  "keywords": [
    "d3",
    "svg",
    "react",
    "charts",
    "graphs",
    "animation",
    "transitions",
    "interpolation"
  ],
  "author": "Steven Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sghall/resonance/issues"
  },
  "homepage": "https://github.com/sghall/resonance#readme",
  "scripts": {
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:copy-files && npm run build:umd:dev && npm run build:umd:min",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:umd:dev": "webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack",
    "clean:build": "rimraf build",
    "lint": "eslint src docs/src test --quiet --cache && echo \"eslint: no lint errors\"",
    "prebuild": "npm run clean:build",
    "test": "cross-env NODE_ENV=test babel-node test/index.js",
    "test:coverage": "cross-env NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli.js cover test/index.js",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:unit": "cross-env NODE_ENV=test babel-node test/index.js unit",
    "test:unit:watch": "cross-env NODE_ENV=test babel-node test/watch.js unit",
    "flow": "flow"
  },
  "peerDependencies": {
    "prop-types": "^15.5.7",
    "react": "^15.5.3"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "d3-color": "^1.0.2",
    "d3-interpolate": "^1.1.3",
    "d3-timer": "^1.0.4",
    "lodash.once": "^4.1.1",
    "simple-assign": "^0.1.0"
  },
  "devDependencies": {
    "app-module-path": "^2.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.44.0",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^10.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.13",
    "prop-types": "^15.5.7",
    "react": "^15.5.3",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.5.3",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0"
  }
}
